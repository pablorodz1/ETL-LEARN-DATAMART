INNER JOIN DATAMART_NORTHWND.dbo.DIM_CLIENTE AS dc 
    ON sv.Cliente_Codigo = dc.Cliente_Codigo
INNER JOIN DATAMART_NORTHWND.dbo.DIM_EMPLEADO AS dm 
    ON sv.Empleado_codigo = dm.Empleado_Codigo
INNER JOIN DATAMART_NORTHWND.dbo.DIM_PRODUCTO AS dp 
    ON sv.Producto_Codigo = dp.Producto_Codigo
INNER JOIN DATAMART_NORTHWND.dbo.DIM_PROVEEDOR AS df 
    ON sv.Proveedor_Codigo = df.Proveedor_Codigo
INNER JOIN DATAMART_NORTHWND.dbo.DIM_Tiempo AS dt 
   on sv.Ventas_OrderDate =  dt.Tiempo_FechaActual</property>
